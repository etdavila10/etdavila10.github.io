---
import Project from './Project.astro';
const projects = [
  {
    id: 'matrix-manipulator',
    title: 'Matrix Manipulator',
    authors: ['Tara Gomes', 'Chris'],
    githubLink: 'https://github.com/etdavila10/MatrixManipulator'
  },
  {
    id: 'arxiv-game',
    title: 'ArXiv Game',
    authors: ['Aaron Li'],
    mainLink: 'https://tranquil-starship-92a3ed.netlify.app/'
  },
]
// const { id, title, authors, githubLink, mainLink } = project;
---

<div id='coding-section' data-link-name='Coding' class='section mt-12'>
  <div id='coding-header' class='font-display w-full h-5 mb-11 border-b border-slate-700 text-left'>
    <span class='text-4xl font-bold mb-6 bg-gray-100 pr-10'>
      Coding
    </span>
  </div>
  <div id='coding-interest-container' data-link-name='Coding Interests' class='sub-section mb-6'>
    <h3 class='text-2xl mb-3 font-display underline'>Coding Interests</h3>
    <p>
    </p>
  </div>
  <div id='publications-container' data-link-name='Publications' class='sub-section'>
    <h3 class='text-2xl mb-3 font-display underline'>Publications</h3>
    { projects && projects.map(project => <Project project={ project } />) }
  </div>
</div>
